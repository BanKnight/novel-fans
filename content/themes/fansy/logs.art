{{extend './layouts/main.art'}}

{{block 'bar_body'}}
    <div class="weui_panel_bd">
        <div class="weui_cells_title">日志</div>

        <div class="weui_cells">
            {{each logs log}}
                {{include './partials/log_card.art' log}}
            {{/each}}
        </div>
 
    </div>

{{/block}}

{{block 'page_footer'}}

    <script>
        turn_bar_on("logs")
        scroll_bar_body_bottom()
    </script>

{{/block}}